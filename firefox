		                }
	                    else 
	                    {
	                        if(cols.length>=1)   
		                        tb=tb+'<tr><td>'+cols[1].innerHTML +'</td><td><input name='+ i +' type=checkbox  checked=true></td>'
                        }
		                
		                
		                    
		                if(i<rows.length)
		                {
		                    i++;
		                    cols = rows[i].getElementsByTagName("td"); 
		                    
		                    if(rows[i].style.display == 'none')
	                        {
	                            if(cols.length>=1)   
		                            tb=tb+'<td>'+cols[1].innerHTML +'</td><td><input name='+ i +' type=checkbox></td>'
