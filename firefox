    <script type="text/javascript" language="javascript">
        $(document).ready(function() {

            // Reset Font Size
            //var originalFontSize = $('html').css('font-size');
            //alert(window.name);
            var fonsize1 = '';
            var originalFontSize = window.name; //'' + newFontSize + 'px';
            $('html').css('font-size', originalFontSize);

            $(".resetFont").click(function() {
                window.name = originalFontSize + "px";
                //                '';
                $('html').css('font-size', originalFontSize);
            });
            // Increase Font Size
            $(".increaseFont").click(function() {
                var currentFontSize = $('html').css('font-size');
                if (parseFloat(currentFontSize, 10) > 18)
                { return false; }
                var currentFontSizeNum = parseFloat(currentFontSize, 10);
                var newFontSize = currentFontSizeNum * 1.2;
                window.name = newFontSize + "px";
                //                '';
                $('html').css('font-size', newFontSize);
                return false;
            });
            // Decrease Font Size
            $(".decreaseFont").click(function() {
                var currentFontSize = $('html').css('font-size');
                if (parseFloat(currentFontSize, 10) < 14)
                { return false; }
                var currentFontSizeNum = parseFloat(currentFontSize, 10);
                var newFontSize = currentFontSizeNum * 0.8;
                window.name = newFontSize + "px";
                //                '';
                $('html').css('font-size', newFontSize);

            });
            $(".BlackFont").click(function() {
                $('body').css('background-color', '#000000');
                $('table').css('background-color', '#000000');
                $('body').css('color', '#FBF202');
                $('table').css('color', '#FBF202');

                return false;
            });
        });
    </script>
    
