        function filterRows()
        {
            divFilter.style.display = 'none';
            
            var table = document.getElementById('tableReportTable');  
            var rows = table.getElementsByTagName("tr"); 
            
            var inputs = document.getElementsByTagName("input"); //or document.forms[0].elements;   
            var cbs = []; //will contain all checkboxes   
            var checked = []; //will contain all checked checkboxes   
            for (var i = 0; i < inputs.length; i++) 
            {   
              if (inputs[i].type == "checkbox") 
              {   
                if(inputs[i].checked==false)
                    rows[inputs[i].name].style.display = 'none';
                else
                    rows[inputs[i].name].style.display = 'block';
              }   
            }  
            var j=0;
            

            var tot=new Array();
            tot[0]=0;
            tot[1]=0;
            tot[2]=0;
            tot[3]=0;
            tot[4]=0;
            tot[5]=0;
            tot[6]=0;
            tot[7]=0;
            tot[8]=0;
            tot[9]=0;
            tot[10]=0;
            tot[11]=0;
            tot[12]=0;
            tot[13]=0;
            tot[14]=0;
            tot[15]=0;
            tot[16]=0;
            tot[17]=0;
            tot[18]=0;
            tot[19]=0;
            tot[20]=0;
            tot[21]=0;
            tot[22]=0;
            tot[23]=0;
            tot[24]=0;
            tot[25]=0;
            tot[26]=0;
            tot[27]=0;
            tot[28]=0;
            tot[29]=0;
            tot[30]=0;
            tot[31]=0;
            tot[32]=0;
            tot[33]=0;
            tot[34]=0;
            tot[35]=0;
            tot[36]=0;
            tot[37]=0;
            tot[38]=0;
            tot[39]=0;
            tot[40]=0;
            
            
            for(var i = 1; i < rows.length; i++)
	        { 
	        
	            if(rows[i].style.display == 'block')
	            {
	                j++;
	                
	                
	                var cols = rows[i].getElementsByTagName("td"); 
	                if(cols.length>=1)
	                    cols[0].innerHTML=j;
	                    
	                    
	                for(var c = 1; c < cols.length; c++)
                    {
                        
                        if (cols[c].childNodes.length > 1) 
                        {
                            
                            var child = cols[c].firstChild;
                            tot[c]=tot[c]+parseFloat(child.innerHTML);
                            
                        }
                        else
                        {
                            tot[c]=tot[c]+parseFloat(cols[c].innerHTML);
                            
                        }
                    }    
	            }
	            
	        } 
	        
	        var colstotal = rows[rows.length-1].getElementsByTagName("th");     

	        for(var t = 1; t < colstotal.length; t++)
            {
                var spans = colstotal[t].getElementsByTagName("span"); 
                
                try
                {
                    if(parseFloat(spans[0].innerHTML).toString().toLowerCase!="nan")
                    {
                    
                        spans[0].innerHTML=Math.round(tot[t+1]*100)/100;
                    }
                }
                catch(err){}
            }
	        
        }
        
